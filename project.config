[access "refs/for/refs/*"]
	pushMerge = group fuel-plugin-contrail-core

[access "refs/heads/*"]
	abandon = group fuel-plugin-contrail-core
	create = group fuel-plugin-contrail-core
	label-Code-Review = -2..+2 group fuel-plugin-contrail-core
	label-Verified = -1..+1 group fuel-plugins-ci
	label-Workflow = -1..+1 group fuel-plugin-contrail-core
	labelAs-Code-Review = -1..+1 group user/Fuel TPI CI (fuel-tpi-ci)
	labelAs-Verified = -1..+1 group user/Fuel TPI CI (fuel-tpi-ci)

[access "refs/tags/*"]
	pushSignedTag = group fuel-plugin-contrail-core

[receive]
	requireChangeId = true
	requireContributorAgreement = true

[submit]
	mergeContent = true
