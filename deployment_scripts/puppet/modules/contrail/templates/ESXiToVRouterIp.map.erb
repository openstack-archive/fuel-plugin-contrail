<%-
network_metadata=scope.lookupvar('contrail::network_metadata')
contrail_esxi_info=scope.lookupvar('contrail::contrail_esxi_info')
-%>
<%- contrail_esxi_info.each do | host | -%>
<%- private_ip=scope.function_get_node_private_ip([network_metadata, host['admin_ip']]) -%>
<%- if private_ip -%>
<%= host['host_ip'] %>:<%= private_ip %>
<%- end -%>
<%- end -%>
