[DEFAULTS]
ifmap_server_ip=<%= scope.lookupvar('contrail::address') %>
ifmap_server_port=8443
ifmap_username=api-server
ifmap_password=api-server
cassandra_server_list=<%= scope.lookupvar('contrail::contrail_db_ips').map{ |ip| "#{ip}:9160" }.join(' ') %>
listen_ip_addr=0.0.0.0
listen_port=9100
log_file=/var/log/contrail/contrail-api.log
log_local=1
log_level=SYS_NOTICE
disc_server_ip=<%= scope.lookupvar('contrail::contrail_private_vip') %>
disc_server_port=5998
zk_server_ip=<%= scope.lookupvar('contrail::contrail_db_ips').map{ |ip| "#{ip}:2181" }.join(',') %>
redis_server_ip=$__contrail_redis_ip__
rabbit_server=<%= scope.lookupvar('contrail::contrail_private_vip') %>
rabbit_port=5673
list_optimization_enabled=True
auth = keystone
aaa_mode=cloud-admin
cloud_admin_role=admin
rabbit_user = nova
rabbit_password = <%= scope.lookupvar('contrail::rabbit_password') %>

[SECURITY]
use_certs=False
keyfile=/etc/contrail/ssl/private_keys/apiserver_key.pem
certfile=/etc/contrail/ssl/certs/apiserver.pem
ca_certs=/etc/contrail/ssl/certs/ca.pem
